# luas persegi panjang view

package MENU;

import KELAS.luasPersegiPanjang;
public class PersegiPanjangView extends javax.swing.JPanel {

    public PersegiPanjangView() {
        initComponents();
    }
     private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
    luasPersegiPanjang pp = new luasPersegiPanjang();
    int panjang = Integer.parseInt(jTextField1.getText());
    int lebar = Integer.parseInt(jTextField2.getText());
    int hasil = pp.lpp(panjang, lebar);
    jLabelHasil2.setText(""+hasil);
    
    }                                        


    // Variables declaration - do not modify                     
    private javax.swing.JButton jButton1;
    private javax.swing.JLabel jLabel1;
    private javax.swing.JLabel jLabel2;
    private javax.swing.JLabel jLabelHasil2;
    private javax.swing.JTextField jTextField1;
    private javax.swing.JTextField jTextField2;
    // End of variables declaration                   
}
