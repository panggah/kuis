# luas persegi view

package MENU;

import KELAS.luasPersegi;

public class PersegiView extends javax.swing.JPanel {

    public PersegiView() {
        initComponents();
    }
    
    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
    luasPersegi p = new luasPersegi();
    int sisi = Integer.parseInt(jTextField1.getText());    
    int hasil = p.persegi(sisi);
    jLabelHasil1.setText(""+hasil);    // TODO add your handling code here:
    }                                        


    // Variables declaration - do not modify                     
    private javax.swing.JButton jButton1;
    private javax.swing.JLabel jLabel1;
    private javax.swing.JLabel jLabelHasil1;
    private javax.swing.JTextField jTextField1;
    // End of variables declaration                   
}
